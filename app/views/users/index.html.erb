<table class="table">
	<thead>
		<tr>
			<th>プロフィール画像</th>
			<th>名前</th>
		</tr>
	</thead>

	<% @users.each do |user| %>
		<tbody>
			<tr>
				<td>
					<img src=<%= user.image %> class = "index_icon_image">
				</td>
				<td>
					<%=link_to (user.name), user_path(user.id) %>
				</td>
			</tr>
		</tbody>
	<% end %>

</table>
