<% @users.each do |user| %>

<table class="table">
	<tr>
		<td>
			<img src=<%= user.image %> class = "index_icon_image">
		</td>
		<td>
			<%= link_to user_path(user.id) do %>
				<%= user.name %>
			<% end %>
		</td>
		<td>フォロー数: <%= user.followings.count %></td>
		<td>フォロワー数: <%= user.followers.count %></td>
	</tr>
</table>

<% end %>